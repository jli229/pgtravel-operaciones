{% extends 'frontend.html.twig' %}

{% block content %}
    <div class="page-title">
        <div class="title_left">
            <h3>{{ 'Rent car types'|trans }} <small>{{ 'view'|trans }}</small></h3>
        </div>
    </div>
    <div class="x_panel">
        <div class="x_title">
            <h2>{{ 'Rent car types'|trans }}</h2>
            <div class="clearfix"></div>
        </div>
        <div class="x_content">
            <dirv class="row">
                <div class="col-xs-12">
                    <label for="" class="control-label">{{ 'Name'|trans }}</label>
                    <div class="form-control">{{ record.name }}</div>
                    <div class="help-block"></div>
                </div>
            </dirv>
        </div>
    </div>
    <div class="pull-right">
        <a href="{{ path('app_rentalcartypes_edit', {'id': record.id}) }}" class="btn btn-primary">{{ 'Edit'|trans }}</a>
        <a href="{{ path('app_rentalcartypes_index') }}" class="btn btn-default">{{ 'Back to list'|trans }}</a>
    </div>
    <div class="clearfix"></div>
{% endblock %}
