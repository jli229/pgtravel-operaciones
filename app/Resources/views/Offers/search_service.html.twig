            </form>
        </div>
        <div class="tab-pane" role="tabpanel" id="tab-transport" style="padding: 16px;">
            <form action="" role="form" id="filter-transport">
                <div class="row">
                    <div class="col-sm-5 form-group">
                        <label class="control-label">{{ 'From'|trans }}</label>
                        <div class="input-group date">
                            <input type="text" class="form-control input-sm datepicker">
                            <span class="input-group-addon">
                                <span class="glyphicon glyphicon-calendar"></span>
                            </span>
                        </div>
                    </div>
                    <div class="col-sm-5 form-group">
                        <label class="control-label">{{ 'To'|trans }}</label>
                        <div class="input-group date">
                            <input type="text" class="form-control input-sm datepicker">
                            <span class="input-group-addon">
                                <span class="glyphicon glyphicon-calendar"></span>
                            </span>
                        </div>
                    </div>
                    <div class="col-sm-2 form-group">
                        <label class="control-label">{{ 'Quantity'|trans }}</label>
                        <select class="form-control input-sm" name="quantity">
                            {% for i in 1..10 %}
                                <option value="{{ i }}">{{ i }}</option>
                            {% endfor %}
                        </select>
                    </div>
                </div>
                <div class="row">
                    <div class="col-sm-offset-10 col-sm-2 form-group">
                        <label class="">
                            <input type="checkbox" name="plushalfday" checked="checked" class="flat">
                            {{ 'Add half day'|trans }}
                        </label>
                    </div>
                </div>
                <div class="row">
                    <div class="table-responsive">
                        <table class="table table-results table-condensed table-striped table-bordered"></table>
                    </div>
                </div>
            </form>
        </div>
        {% for model in models if model.name not in ['hotel', 'car-rental', 'private-house', 'transport'] %}
            <div class="tab-pane" role="tabpanel" id="tab-{{ model.name }}" style="padding: 16px;">
                <form action="" role="form" id="filter-{{ model.name }}">
                    <div class="row row-datepickers">
                        <div class="col-sm-5 form-group">
                            <label class="control-label">{{ 'From'|trans }}</label>
                            <div class="input-group date">
                                <input type="text" class="form-control input-sm datepicker">
                                <span class="input-group-addon">
                                    <span class="glyphicon glyphicon-calendar"></span>
                                </span>
                            </div>
                        </div>
                        <div class="col-sm-5 form-group">
                            <label class="control-label">{{ 'To'|trans }}</label>
                            <div class="input-group date">
                                <input type="text" class="form-control input-sm datepicker">
                                <span class="input-group-addon">
                                    <span class="glyphicon glyphicon-calendar"></span>
                                </span>
                            </div>
                        </div>
                        <div class="col-sm-2 form-group">
                            <label class="control-label">{{ 'Quantity'|trans }}</label>
                            <select class="form-control input-sm" name="quantity">
                                {% for i in 1..10 %}
                                    <option value="{{ i }}">{{ i }}</option>
                                {% endfor %}
                            </select>
                        </div>
                    </div>
                    <div class="row">
                        <div class="table-responsive results"></div>
                    </div>
                </form>
            </div>
        {% endfor %}
    </div>
</div>
